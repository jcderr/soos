#!/bin/bash

DESCRIPTION="Internal docker script to perform the migration"
USAGE="Do not call manually"
HELP=""

main () {
    pip_install_requirements
    
    info "Migrating database."
    $MANAGECMD syncdb --noinput
    
    # if you're using south, uncomment this line
    #$MANAGECMD migrate
}

source "$(dirname $0)/../sooz-common"

